@startuml ATM_DomainModel

class Card {
  +cardNumber
  +status
}

class Transaction {
  +transactionType
  +amount
  +timestamp
}

class ATM {
  +location
  +status
}

class Receipt {
  +transactionDetails
  +timestamp
}

class PhysicalMoney {
  +denomination
  +quantity
}

Card "1" -- "1" Transaction : associated with >
Transaction "1" -- "1" Receipt : generates >
ATM "1" -- "*" Transaction : processes >
ATM "1" -- "*" PhysicalMoney : dispenses >

@enduml